  "scripts": {
    "build": "vite build",
    "storybook": "storybook dev -p 8008",
    "storybook:build": "STORYBOOK=true storybook build --quiet",
    "test:storybook": "test-storybook --url http://localhost:8008 --maxWorkers 4",
    "test:storybook:visual": "./packages/visual-regressions/bin/take-and-check-in-docker.sh",
    "test:storybook:visual:update": "./packages/visual-regressions/bin/take-and-update-in-docker.sh",
    "test:storybook:visual:ci": "./packages/visual-regressions/bin/take-and-check-in-docker-ci.sh",
    "test": "jest -w 4",
    "test:watch": "jest --watch",
    "types": "tsc",
    "lint": "eslint 'src/**/*.{js,ts,tsx,md,mdx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx,md,mdx}' --fix",
    "lint:css": "stylelint 'src/**/*.{js,ts,tsx}'",
    "semantic-release": "semantic-release",
    "betterer": "betterer -t ./tsconfig.json -c .betterer/.betterer.ts -r .betterer/.betterer.results --strict",
    "betterer:precommit": "betterer precommit -t ./tsconfig.json -c .betterer/.betterer.ts -r .betterer/.betterer.results --strict",
    "betterer:ci": "betterer ci -t ./tsconfig.json -c .betterer/.betterer.ts -r .betterer/.betterer.results --strict",
    "prepare": "husky install"
  },
